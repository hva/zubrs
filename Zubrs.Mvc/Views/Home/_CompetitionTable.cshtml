@model IEnumerable<Zubrs.Models.Rank>

<table class="@ViewData["class"]">
    <thead>
        <tr>
            <td></td>
            <td></td>
            <td>И</td>
            <td>В</td>
            <td>П</td>
            <td>%</td>
        </tr>
    </thead>
    <tbody>
        @{ int i = 1; }
        @foreach (var x in @Model)
        {
            <tr>
                <td>@(i++).</td>
                <td>@x.Team.Title</td>
                <td>@x.GamesPlayed</td>
                <td>@x.GamesWon</td>
                <td>@(x.GamesPlayed - x.GamesWon)</td>
                <td></td>
            </tr>
        }
    </tbody>
</table>
